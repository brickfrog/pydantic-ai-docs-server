{
  "name": "pydantic-ai-docs-server",
  "version": "0.0.46",
  "description": "MCP server for accessing pydantic-ai documentation, changelogs, and news.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "pydantic-ai-docs-server": "dist/stdio.js"
  },
  "files": [
    "dist",
    "docs/raw",
    "docs/organized",
    "README.md",
    "bin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brickfrog/pydantic-ai-docs-server.git"
  },
  "keywords": [
    "pydantic",
    "documentation",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.7.0",
    "date-fns": "^4.1.0",
    "exit-hook": "^4.0.0",
    "fastmcp": "^1.20.4",
    "jsdom": "^26.0.0",
    "turndown": "^7.1.2",
    "tylerbarnes-fastmcp-fix": "^1.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hono/node-server": "^1.13.8",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.24",
    "@types/turndown": "^5.0.5",
    "@wong2/mcp-cli": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "hono": "^4.7.4",
    "tsup": "^8.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^3.0.9"
  },
  "scripts": {
    "build": "tsup --config tsup.config.js",
    "sync": "node sync-docs.js"
  }
}
